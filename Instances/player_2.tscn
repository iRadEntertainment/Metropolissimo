[sub_resource type="GDScript" id=47]
script/source = "#player_draw_debug

extends Node2D

var col = Color(0.8 , 0.15 , 0.15)

func _ready():
	show()

var time = 0
func _process(delta):
	update()
	time += delta
	
	if time >= 0.1:
		$vec_mov.text = str(stepify(get_parent().vec_mov.x,0.1), \",\" , stepify(get_parent().vec_mov.y,0.1))
		time=0

func _draw():
	if data_mng.cfg_debug_mode:
		var vec = get_parent().vec_mov/10
		draw_circle(Vector2(),2,col)
		draw_line(Vector2(),vec,col,2,true)
		
		if !$vec_mov.visible: $vec_mov.visible = true
		
	elif $vec_mov.visible: $vec_mov.visible = false"

